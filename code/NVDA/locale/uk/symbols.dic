complexSymbols:
# identifier	regexp
decimal point	(?<=\d)\,(?=\d)

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]
# Complex symbols
. sentence ending	крапка	# . в кінці речення
! sentence ending	знак оклику	# ! в кінці речення
? sentence ending	знак питання	# ? в кінці речення
; phrase ending	крапка з комою	# ; в кінці фрази
: phrase ending	двокрапка	# : в кінці фрази
decimal point	кома	# десятковий дріб

# Whitespace
\0	чисто
\t	таб
\n	переведення рядка
\f	розрив сторінки
\r	повернення каретки
 	пробіл
 	пробіл	# нерозривний пробіл
in-word '	апостроф
negative number	мінус

