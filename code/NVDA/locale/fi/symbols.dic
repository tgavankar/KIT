complexSymbols:
# identifier	regexp
# Sentence endings.
... sentence ending	(?<=[^\s.])(\s\.|\.)\.\.(?=[\"'”’)\s]|$)
… sentence ending	(?<=[^\s…])\…(?=[\"'”’)\s]|$)
! sentence ending	(?<=[^\s!])(\s\!|\!)(?=[\"'”’)\s]|$)
? sentence ending	(?<=[^\s!])(\s\?|\?)(?=[\"'”’)\s]|$)
# Phrase endings.
; phrase ending	(?<=[^\s!])(\s\;|\;)(?=[\"'”’)\s]|$)
: phrase ending	(?<=[^\s!])(\s\:|\:)(?=[\"'”’)\s]|$)
# Others
decimal point	(?<=\d)\,(?=\d)
percent	(?<=\d)\ ?\%(?!\w)
dates	(((0?[1-9]|[12]\d|3[01])\.(0?[13578]|1[02])\.((1[6-9]|[2-9]\d)?\d{2}|\d))|((0?[1-9]|[12]\d|30)\.(0?[13456789]|1[012])\.((1[6-9]|[2-9]\d)?\d{2}|\d))|((0?[1-9]|1\d|2[0-8])\.]0?2\.]((1[6-9]|[2-9]\d)?\d{2}|\d))|(29\.0?2\.((1[6-9]|[2-9]\d)?(0[48]|[2468][048]|[13579][26])|((16|[2468][048]|[3579][26])00)|00|[048])))
euro	^(0|(([1-9]{1}|[1-9]{1}[0-9]{1}|[1-9]{1}[0-9]{2}){1}(\ [0-9]{3}){0,})),(([0-9]{2})|\-\-)([\ ]{1})(€){1}$
thousands	(^(((\d)|(\d\d)|(\d\d\d))(\xA0|\x20))*((\d)|(\d\d)|(\d\d\d))([,.]\d*)?$)

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. sentence ending	piste	all	always
! sentence ending	huuto	all	always
? sentence ending	kysymys	all	always
... sentence ending	piste piste piste	all	always
… sentence ending	piste piste piste	all	always
; phrase ending	puolipiste	most	always
: phrase ending	kaksoispiste	most	always
